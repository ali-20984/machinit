#!/usr/bin/env bash
# Minimal black completion â€” common subcommands and options

#compdef black

local -a cmds
cmds=(
  'check:check if files would be reformatted'
  'format:reformat files'
  '--version:print version'
  '--help:show help'
  '--line-length#:wrap at column'
  '--target-version:specify target Python version'
  '--safe:do not apply changes'
  '--fast:skip experimental safety checks'
)

_describe 'black command' cmds && return 0

_files
