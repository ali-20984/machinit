#!/usr/bin/env bash
# Auto-generated completion for git (from --help output)
#compdef git

local -a commands
commands=(
  'add:add subcommand'
  'backfill:backfill subcommand'
  'bisect:bisect subcommand'
  'branch:branch subcommand'
  'clone:clone subcommand'
  'commit:commit subcommand'
  'diff:diff subcommand'
  'fetch:fetch subcommand'
  'grep:grep subcommand'
  'init:init subcommand'
  'log:log subcommand'
  'merge:merge subcommand'
  'mv:mv subcommand'
  'pull:pull subcommand'
  'push:push subcommand'
  'rebase:rebase subcommand'
  'reset:reset subcommand'
  'restore:restore subcommand'
  'rm:rm subcommand'
  'show:show subcommand'
  'status:status subcommand'
  'switch:switch subcommand'
  'tag:tag subcommand'
)
_describe 'command' commands && return 0
local -a opts
opts=(
  '--bare:--bare option'
  '--config-env=:--config-env= option'
  '--exec-path:--exec-path option'
  '--git-dir=:--git-dir= option'
  '--help:--help option'
  '--html-path:--html-path option'
  '--info-path:--info-path option'
  '--man-path:--man-path option'
  '--namespace=:--namespace= option'
  '--no-advice:--no-advice option'
  '--no-lazy-fetch:--no-lazy-fetch option'
  '--no-optional-locks:--no-optional-locks option'
  '--no-pager:--no-pager option'
  '--no-replace-objects:--no-replace-objects option'
  '--paginate:--paginate option'
  '--version:--version option'
  '--work-tree=:--work-tree= option'
  '-C:-C option'
  '-P:-P option'
  '-a:-a option'
  '-c:-c option'
  '-g:-g option'
  '-h:-h option'
  '-p:-p option'
  '-v:-v option'
)
_describe 'option' opts && return 0
_files
