#!/usr/bin/env bash
# vcpkg completion stub â€” minimal options
#compdef vcpkg

local -a cmds
cmds=(
  'install:Install a package'
  'remove:Remove a package'
  'search:Search packages'
  'list:List installed packages'
  'help:Show help'
  'integrate:Integrate with an IDE or environment'
  'export:Export a list of dependencies'
  'update:Update package list'
)

_describe 'vcpkg subcommand' cmds && return 0

_files
