#!/usr/bin/env bash
# Minimal VS Code CLI `code` completion â€” common flags

#compdef code

local -a opts
opts=(
  '--install-extension:install an extension'
  '--uninstall-extension:uninstall an extension'
  '--list-extensions:list installed extensions'
  '--version:print version'
  '--disable-extensions:do not load extensions'
  '--help:show help'
)

_describe 'code option' opts && return 0

_files
